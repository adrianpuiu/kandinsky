        "import os\n",
        "os.environ['CUDA_VISIBLE_DEVICES'] = '0'\n",
        "from copy import deepcopy\n",
        "\n",
        "import torch\n",
        "import torch.nn as nn\n",
        "from matplotlib import pyplot as plt\n",
        "\n",
        "from kandinsky2 import get_kandinsky2"
      ],
      "metadata": {
        "id": "CdXVbpvIt6QZ"
      },
      "execution_count": 3,
      "outputs": []
